<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#9AD5EA"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/rate"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="229dp"
        android:layout_marginLeft="229dp"
        android:layout_marginTop="181dp"
        android:ems="10"
        android:hint="Commission rate"
        android:inputType="number"
        android:textColor="#5269EA"
        android:textColorHint="#5269EA" />

    <EditText
        android:id="@+id/sale"
        android:layout_width="99dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="230dp"
        android:layout_marginLeft="230dp"
        android:layout_marginTop="112dp"
        android:ems="10"
        android:hint="total sales"
        android:inputType="number"
        android:textColor="#5269EA"
        android:textColorHint="#5269EA" />

    <EditText
        android:id="@+id/salary"
        android:layout_width="99dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="90dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="182dp"
        android:ems="10"
        android:hint="base salary"
        android:inputType="number"
        android:textColor="#5269EA"
        android:textColorHint="#5269EA" />

    <EditText
        android:id="@+id/name"
        android:layout_width="99dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="90dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="112dp"
        android:ems="10"
        android:hint="name"
        android:inputType="text"
        android:textColor="#5269EA"
        android:textColorHint="#5269EA" />

    <EditText
        android:id="@+id/id"
        android:layout_width="99dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="90dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="43dp"
        android:ems="10"
        android:hint="ID"
        android:inputType="number"
        android:textColor="#5269EA"
        android:textColorHint="#4F66EA" />

    <Button
        android:id="@+id/insrtbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginBottom="176dp"
        android:background="@android:drawable/ic_menu_add"
        android:textColor="#B5360E" />

    <Button
        android:id="@+id/dltbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="178dp"
        android:background="@android:drawable/ic_menu_delete"
        android:textColor="#B5360E" />

    <Button
        android:id="@+id/srchbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="120dp"
        android:background="@android:drawable/ic_menu_search"
        android:textColor="#B5360E" />

    <Button
        android:id="@+id/editbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="17dp"
        android:layout_marginLeft="17dp"
        android:layout_marginBottom="119dp"
        android:background="@android:drawable/ic_menu_edit"
        android:textColor="#B5360E" />

    <Button
        android:id="@+id/dspbtn"


        android:layout_width="183dp"
        android:layout_height="101dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="115dp"
        android:layout_marginEnd="113dp"

        android:layout_marginRight="113dp"
        android:layout_marginBottom="122dp"
        android:background="@android:drawable/ic_menu_view"
        android:textColor="#B5360E" />

    <TextView
        android:id="@+id/srchView"

        android:layout_width="401dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginBottom="54dp"
        android:textColor="#308695"
        android:textSize="16sp" />

    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="71dp"
        android:layout_marginRight="71dp">

        <RadioButton
            android:id="@+id/F"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="FEMALE"
            android:textColor="#5269EA" />

        <RadioButton
            android:id="@+id/M"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="MALE"
            android:textColor="#5269EA" />
    </RadioGroup>

</RelativeLayout>